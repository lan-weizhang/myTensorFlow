   "source": [
    "import tensorflow as tf\n",
    "\n",
    "a = tf.constant([1,2,3,4])\n",
    "b = tf.constant( [2,3,4,5])\n",
    "c = tf.square(a-b)\n",
    "\n",
    "\n",
    "init = tf.global_variables_initializer()\n",
    "with tf.Session() as sess:\n",
    "    #变量初始化\n",
    "    sess.run(init)\n",
    "    c_value = sess.run(c)\n",
    "    print(c_value)"
   ]
